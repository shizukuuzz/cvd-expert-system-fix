        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Pasien"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Rata-rata gula darah 2-3 bulan. Satuan: %. Normal: &lt;5.7%, Prediabetes: 5.7-6.4%, Diabetes: &gt;=6.5%</rdfs:comment>
        <rdfs:label>Hemoglobin A1c (HbA1c)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.cvd-expert-system.org/ontology#memilikiIMT -->

    <owl:DatatypeProperty rdf:about="http://www.cvd-expert-system.org/ontology#memilikiIMT">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Pasien"/>
