    <!-- http://www.cvd-expert-system.org/ontology#Amlodipine -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Amlodipine">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#CCB"/>
        <hasDescription>Calcium Channel Blocker (CCB) golongan dihidropiridin yang melebarkan pembuluh darah. Indikasi: hipertensi, angina. Keunggulan: efektif pada lansia, tidak mempengaruhi denyut jantung, aman pada asma. Efek samping: edema pergelangan kaki (sering), sakit kepala, flushing. Dapat dikombinasi dengan ACE-I/ARB untuk efek sinergis.</hasDescription>
        <hasDisplayName>Amlodipine</hasDisplayName>
        <hasDose>5mg</hasDose>
        <hasDrugClass>CCB</hasDrugClass>
        <hasFrequency>1x sehari</hasFrequency>
    </owl:NamedIndividual>
    


