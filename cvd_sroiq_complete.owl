        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Pasien"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Biomarker gagal jantung. Satuan: pg/mL. Normal: &lt;100. Elevated: &gt;100</rdfs:comment>
        <rdfs:label>BNP</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.cvd-expert-system.org/ontology#memilikiBeratBadan -->

    <owl:DatatypeProperty rdf:about="http://www.cvd-expert-system.org/ontology#memilikiBeratBadan">
