        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Berat badan dalam kilogram (kg)</rdfs:comment>
        <rdfs:label>Berat Badan</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.cvd-expert-system.org/ontology#memilikiCHA2DS2VASc -->

    <owl:DatatypeProperty rdf:about="http://www.cvd-expert-system.org/ontology#memilikiCHA2DS2VASc">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Pasien"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Risiko stroke untuk AF. Skor 0: rendah, 1: sedang, &gt;=2: tinggi (perlu antikoagulan)</rdfs:comment>
        <rdfs:label>Skor CHA2DS2-VASc</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.cvd-expert-system.org/ontology#memilikiDenyutJantung -->

    <owl:DatatypeProperty rdf:about="http://www.cvd-expert-system.org/ontology#memilikiDenyutJantung">
