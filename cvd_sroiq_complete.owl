        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Pasien"/>
        <rdfs:range rdf:resource="http://www.cvd-expert-system.org/ontology#Penyakit"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.cvd-expert-system.org/ontology#memiliki"/>
            <rdf:Description rdf:about="http://www.cvd-expert-system.org/ontology#menyebabkan"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


