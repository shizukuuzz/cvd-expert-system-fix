        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Penyakit"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.cvd-expert-system.org/ontology#FaktorRisiko"/>
                    <rdf:Description rdf:about="http://www.cvd-expert-system.org/ontology#Penyakit"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


