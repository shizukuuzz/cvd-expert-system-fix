    <!-- http://www.cvd-expert-system.org/ontology#memilikiASCVDScore -->

    <owl:DatatypeProperty rdf:about="http://www.cvd-expert-system.org/ontology#memilikiASCVDScore">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Pasien"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Probabilitas kejadian kardiovaskular dalam 10 tahun. Satuan: %. Rendah: &lt;5%, Borderline: 5-7.5%, Sedang: 7.5-20%, Tinggi: &gt;=20%</rdfs:comment>
        <rdfs:label>Skor Risiko ASCVD 10-Tahun</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.cvd-expert-system.org/ontology#memilikiBNP -->

    <owl:DatatypeProperty rdf:about="http://www.cvd-expert-system.org/ontology#memilikiBNP">
