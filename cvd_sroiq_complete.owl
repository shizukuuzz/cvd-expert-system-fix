        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Pasien"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Heart rate dalam beats per minute (bpm). Normal: 60-100 bpm</rdfs:comment>
        <rdfs:label>Denyut Jantung (HR)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.cvd-expert-system.org/ontology#memilikiEjectionFraction -->

    <owl:DatatypeProperty rdf:about="http://www.cvd-expert-system.org/ontology#memilikiEjectionFraction">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Pasien"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Persentase darah dipompa per kontraksi. Satuan: %. Normal: 55-70%, HFpEF: &gt;=50%, HFmrEF: 41-49%, HFrEF: &lt;=40%</rdfs:comment>
        <rdfs:label>Fraksi Ejeksi (EF)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.cvd-expert-system.org/ontology#memilikiGFR -->

    <owl:DatatypeProperty rdf:about="http://www.cvd-expert-system.org/ontology#memilikiGFR">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Pasien"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Laju filtrasi glomerulus. Satuan: mL/min/1.73m2. Normal: &gt;=90, CKD Stage 2: 60-89, Stage 3: 30-59, Stage 4: 15-29, Stage 5: &lt;15</rdfs:comment>
        <rdfs:label>eGFR (Estimated GFR)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.cvd-expert-system.org/ontology#memilikiGulaDarahPuasa -->

    <owl:DatatypeProperty rdf:about="http://www.cvd-expert-system.org/ontology#memilikiGulaDarahPuasa">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Pasien"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Kadar glukosa setelah puasa 8 jam. Satuan: mg/dL. Normal: &lt;100, Prediabetes: 100-125, Diabetes: &gt;=126</rdfs:comment>
        <rdfs:label>Gula Darah Puasa (FBG)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.cvd-expert-system.org/ontology#memilikiGulaDarahSewaktu -->

    <owl:DatatypeProperty rdf:about="http://www.cvd-expert-system.org/ontology#memilikiGulaDarahSewaktu">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Pasien"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Kadar glukosa kapan saja. Satuan: mg/dL. Diabetes jika &gt;=200 + gejala</rdfs:comment>
        <rdfs:label>Gula Darah Sewaktu</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.cvd-expert-system.org/ontology#memilikiHASBLED -->

    <owl:DatatypeProperty rdf:about="http://www.cvd-expert-system.org/ontology#memilikiHASBLED">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.cvd-expert-system.org/ontology#Pasien"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
