    <!-- http://www.cvd-expert-system.org/ontology#Aspirin -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Aspirin">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#Antiplatelet"/>
        <hasDescription>Antiplatelet yang menghambat agregasi trombosit dengan menghambat COX-1. Indikasi: pencegahan sekunder serangan jantung/stroke (WAJIB), sindrom koroner akut, pasca PCI/stent. Dosis kardiovaskular: 75-100mg/hari. Efek samping: perdarahan GI, tukak lambung. Gunakan bersama PPI jika risiko GI tinggi. TIDAK untuk pencegahan primer rutin.</hasDescription>
        <hasDisplayName>Aspirin</hasDisplayName>
        <hasDose>100mg</hasDose>
        <hasDrugClass>Antiplatelet</hasDrugClass>
        <hasFrequency>1x sehari</hasFrequency>
    </owl:NamedIndividual>
    


