    <!-- http://www.cvd-expert-system.org/ontology#Ramipril -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Ramipril">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#ACEInhibitor"/>
        <hasDisplayName>Ramipril</hasDisplayName>
        <hasDose>5mg</hasDose>
        <hasDrugClass>ACE Inhibitor</hasDrugClass>
        <hasFrequency>1x sehari</hasFrequency>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_AktivitasFisik150 -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_AktivitasFisik150">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiBeratBadan"/>
        <hasCategory>Berat Badan</hasCategory>
        <hasDescription>Tingkatkan aktivitas fisik minimal 150 menit/minggu (untuk maintenance) atau 300 menit/minggu (untuk penurunan BB). Kombinasikan aerobik dan latihan kekuatan.</hasDescription>
        <hasDisplayName>Aktivitas Fisik 150-300 Menit/Minggu</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_BatasiCairan -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_BatasiCairan">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiGagalJantung"/>
        <hasCategory>Gagal Jantung</hasCategory>
        <hasDescription>Batasi asupan cairan total (termasuk sup, buah berair) 1.5-2 liter per hari untuk mencegah overload cairan dan memburuknya gejala gagal jantung.</hasDescription>
        <hasDisplayName>Batasi Cairan 1.5-2 Liter/Hari</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_BatasiGaram -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_BatasiGaram">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiTekananDarah"/>
        <hasCategory>Tekanan Darah</hasCategory>
        <hasDescription>Batasi konsumsi garam kurang dari 2 gram (setara 1 sendok teh) per hari. Hindari makanan olahan, asin, dan MSG. Garam berlebih menyebabkan retensi air dan meningkatkan tekanan darah.</hasDescription>
        <hasDisplayName>Batasi Asupan Garam</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_BatasiKarbohidrat -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_BatasiKarbohidrat">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiGulaDarah"/>
        <hasCategory>Gula Darah</hasCategory>
        <hasDescription>Kurangi konsumsi gula, nasi putih, roti putih, dan makanan manis. Pilih karbohidrat kompleks (nasi merah, oat, ubi). Indeks glikemik rendah membantu kontrol gula darah.</hasDescription>
        <hasDisplayName>Batasi Karbohidrat Sederhana</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_BatasiLemakJenuh -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_BatasiLemakJenuh">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiKolesterol"/>
        <hasCategory>Kolesterol</hasCategory>
        <hasDescription>Kurangi lemak jenuh (daging berlemak, mentega, keju) dan lemak trans (gorengan, margarin). Ganti dengan lemak tidak jenuh (minyak zaitun, alpukat, kacang). Lemak jenuh &lt;7% kalori harian.</hasDescription>
        <hasDisplayName>Batasi Lemak Jenuh dan Trans</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_BerhentiMerokok -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_BerhentiMerokok">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiBerhentiMerokok"/>
        <hasCategory>Merokok</hasCategory>
        <hasDescription>PRIORITAS UTAMA! Merokok meningkatkan risiko serangan jantung 2-4x lipat. Berhenti merokok adalah langkah paling efektif untuk mengurangi risiko kardiovaskular. Dalam 1 tahun berhenti, risiko PJK turun 50%.</hasDescription>
        <hasDisplayName>BERHENTI MEROKOK SEGERA</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_CekGulaDarah -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_CekGulaDarah">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiGulaDarah"/>
        <hasCategory>Gula Darah</hasCategory>
        <hasDescription>Periksa gula darah secara rutin sesuai anjuran dokter. Catat hasil untuk memantau pola. Target HbA1c &lt;7% untuk kebanyakan penderita diabetes.</hasDescription>
        <hasDisplayName>Cek Gula Darah Rutin</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_DietDASH -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_DietDASH">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiTekananDarah"/>
        <hasCategory>Tekanan Darah</hasCategory>
        <hasDescription>Ikuti pola makan DASH (Dietary Approaches to Stop Hypertension): banyak buah, sayur, biji-bijian, produk susu rendah lemak. Kurangi daging merah, gula, dan lemak jenuh. Terbukti menurunkan tekanan darah 8-14 mmHg.</hasDescription>
        <hasDisplayName>Diet DASH</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_KonsumsiIkan -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_KonsumsiIkan">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiKolesterol"/>
        <hasCategory>Kolesterol</hasCategory>
        <hasDescription>Makan ikan berlemak (salmon, makarel, sarden) minimal 2 porsi per minggu. Omega-3 membantu menurunkan trigliserida dan melindungi jantung.</hasDescription>
        <hasDisplayName>Konsumsi Ikan 2x Seminggu</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_KontrolRutin -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_KontrolRutin">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiUmum"/>
        <hasCategory>Umum</hasCategory>
        <hasDescription>Lakukan kontrol rutin ke dokter setiap 3-6 bulan untuk pemantauan kondisi dan penyesuaian pengobatan. Jangan menghentikan atau mengubah dosis obat tanpa konsultasi.</hasDescription>
        <hasDisplayName>Kontrol Rutin ke Dokter</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_MakanTeratur -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_MakanTeratur">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiGulaDarah"/>
        <hasCategory>Gula Darah</hasCategory>
        <hasDescription>Makan teratur 3 kali sehari dengan porsi konsisten. Jangan melewatkan makan, terutama sarapan. Porsi kecil tapi sering lebih baik daripada makan besar jarang.</hasDescription>
        <hasDisplayName>Makan Teratur 3x Sehari</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_OlahragaAerobik -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_OlahragaAerobik">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiTekananDarah"/>
        <hasCategory>Tekanan Darah</hasCategory>
        <hasDescription>Lakukan olahraga aerobik (jalan cepat, berenang, bersepeda) minimal 30 menit per hari, 5 kali per minggu (total 150 menit/minggu). Dapat menurunkan tekanan darah 4-9 mmHg.</hasDescription>
        <hasDisplayName>Olahraga Aerobik Teratur</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_PatuhiObat -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_PatuhiObat">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiUmum"/>
        <hasCategory>Umum</hasCategory>
        <hasDescription>Minum obat secara teratur sesuai resep dokter. Jangan berhenti minum obat meskipun merasa sudah sembuh. Ketidakpatuhan obat adalah penyebab utama komplikasi.</hasDescription>
        <hasDisplayName>Patuhi Pengobatan</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_TargetPenurunanBB -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_TargetPenurunanBB">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiBeratBadan"/>
        <hasCategory>Berat Badan</hasCategory>
        <hasDescription>Targetkan penurunan berat badan 5-10% dari berat awal dalam 6 bulan melalui defisit kalori 500-750 kkal/hari. Penurunan ini sudah cukup signifikan untuk memperbaiki tekanan darah dan gula darah.</hasDescription>
        <hasDisplayName>Target Penurunan BB 5-10%</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_TerapiNikotin -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_TerapiNikotin">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiBerhentiMerokok"/>
        <hasCategory>Merokok</hasCategory>
        <hasDescription>Konsultasikan program berhenti merokok dengan dokter. Pertimbangkan terapi pengganti nikotin (patch, permen karet) atau obat resep (varenicline, bupropion) untuk membantu mengatasi kecanduan.</hasDescription>
        <hasDisplayName>Terapi Pengganti Nikotin</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_TidurCukup -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_TidurCukup">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiUmum"/>
        <hasCategory>Umum</hasCategory>
        <hasDescription>Jaga pola tidur 7-8 jam per malam dengan jadwal teratur. Kurang tidur meningkatkan risiko hipertensi, obesitas, dan diabetes. Atasi sleep apnea jika ada.</hasDescription>
        <hasDisplayName>Tidur Cukup 7-8 Jam</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_TidurKepalaTerangkat -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_TidurKepalaTerangkat">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiGagalJantung"/>
        <hasCategory>Gagal Jantung</hasCategory>
        <hasDescription>Tidur dengan 2-3 bantal atau tinggikan kepala ranjang jika mengalami sesak saat berbaring (orthopnea). Posisi ini mengurangi penumpukan cairan di paru.</hasDescription>
        <hasDisplayName>Tidur dengan Kepala Ditinggikan</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_TimbangBBHarian -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_TimbangBBHarian">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiGagalJantung"/>
        <hasCategory>Gagal Jantung</hasCategory>
        <hasDescription>Timbang berat badan setiap pagi sebelum makan. Kenaikan &gt;1 kg dalam 1 hari atau &gt;2 kg dalam 1 minggu menandakan retensi cairan - segera hubungi dokter!</hasDescription>
        <hasDisplayName>Timbang Berat Badan Setiap Hari</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</hasPriority>
    </owl:NamedIndividual>
    


    <!-- http://www.cvd-expert-system.org/ontology#Rek_TingkatkanSerat -->

    <owl:NamedIndividual rdf:about="http://www.cvd-expert-system.org/ontology#Rek_TingkatkanSerat">
        <rdf:type rdf:resource="http://www.cvd-expert-system.org/ontology#RekomendasiKolesterol"/>
        <hasCategory>Kolesterol</hasCategory>
        <hasDescription>Konsumsi serat larut 10-25 gram per hari dari oat, kacang, buah, sayur. Serat larut mengikat kolesterol di usus dan mencegah penyerapan. Dapat menurunkan LDL 5-10%.</hasDescription>
        <hasDisplayName>Tingkatkan Asupan Serat</hasDisplayName>
        <hasPriority rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</hasPriority>
    </owl:NamedIndividual>
    


